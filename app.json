{
  "expo": {
    "name": "PinoySing",
    "slug": "PinoySing",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo1.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo1.png",
      "resizeMode": "contain",
      "backgroundColor": "#333332"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo1.png",
        "backgroundColor": "#333332"
      },
      "package": "com.rustymaglinte.PinoySing",
      "googleServicesFile": "./assets/google-services.json",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/logo1.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics"
    ],
    "extra": {
      "eas": {
        "projectId": "b83699e6-e832-47bf-9e27-43ef0e6aa0b2"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/b83699e6-e832-47bf-9e27-43ef0e6aa0b2"
    }
  }
}
